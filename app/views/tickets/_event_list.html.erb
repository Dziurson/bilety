<table class ="table table-hover">
  <thead>
    <tr>
      <th>ID Wydarzenia</th>
      <th>Wykonawca</th>      
      <th>Data wydarzenia</th>      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% events.each do |event| %>
      <tr>
        <td><%= event.id %>
        <td><%= link_to event.artist, "/events/#{event.id}" %></td>        
        <td><%= event.event_date %></td>        
        <th colspan="3"></th>
      </tr>
    <% end %>
  </tbody>
</table>